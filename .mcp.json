{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {}
    },
    "serena": {
      "type": "stdio",
      "command": "mise",
      "args": [
        "exec",
        "--",
        "uvx",
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "."
      ],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "bash",
      "args": [
        "-c",
        "npx -y chrome-devtools-mcp@latest --headless=true --isolated=true --executablePath=$(mise run chromium-path)"
      ],
      "env": {}
    },
    "ide": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@anthropic-ai/ide-mcp@latest"
      ],
      "env": {}
    }
  }
}