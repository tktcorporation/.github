{
  "name": "@tktco/create-devenv",
  "version": "0.3.0",
  "description": "Interactive CLI to scaffold development environment templates",
  "type": "module",
  "bin": {
    "create-devenv": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.2",
    "@octokit/rest": "^22.0.1",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "diff": "^8.0.2",
    "giget": "^2.0.0",
    "ignore": "^7.0.5",
    "jsonc-parser": "^3.3.1",
    "pathe": "^2.0.3",
    "tinyglobby": "^0.2.15",
    "ts-pattern": "^5.9.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^22.19.2",
    "@vitest/coverage-v8": "^4.0.15",
    "memfs": "^4.51.1",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.15"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tktcorporation/.github.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
