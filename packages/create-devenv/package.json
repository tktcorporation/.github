{
  "name": "@tktco/create-devenv",
  "version": "0.1.0",
  "description": "Interactive CLI to scaffold development environment templates",
  "type": "module",
  "bin": {
    "create-devenv": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.2",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "giget": "^2.0.0",
    "pathe": "^2.0.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.19.1",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tktcorporation/.github.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
